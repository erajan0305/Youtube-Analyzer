# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
+ nocsrf
GET    /                           controllers.YoutubeAnalyzerController.index(request: Request)
+ nocsrf
POST   /                           controllers.YoutubeAnalyzerController.fetchVideosByKeywords(request: Request)
+ nocsrf
GET    /stats                      controllers.YoutubeAnalyzerController.fetchSimilarityStats()
+ nocsrf√∏
GET    /channel/:id                controllers.YoutubeAnalyzerController.fetchChannelInformation(request: Request, id: String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
